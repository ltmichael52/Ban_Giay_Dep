<!-- start banner Area -->
@model List<ShoesStore.Models.Banner>
<section class="banner-area">
	<div class="container">
		<div class="row fullscreen align-items-center justify-content-start">
			<div class="col-lg-12">
				<div class="active-banner-slider owl-carousel">
					@foreach (var banner in Model)
					{
						<div class="row single-slide align-items-center d-flex">
							<div class="col-lg-5 col-md-6">
								<div class="banner-content">
									<h1>@banner.Tenbanner</h1>
									<h3>@banner.Vitri</h3>
									<div class="add-bag d-flex align-items-center">
										<a class="primary-btn" href="@banner.Link">Shopping Now</a>
									</div>
								</div>
							</div>
							<div class="col-lg-7">
								<div class="banner-img">
									<a href="@banner.Link"><img style="width:300px;height:300px"src="@Url.Content($"~/img/banner/{banner.Vitri}")" alt="Banner Image"></a>
								</div>
							</div>
						</div>
					}
				</div>
			</div>
		</div>
	</div>
</section>
